{
  "compilerOptions": {
    "target": "esnext",
    "module": "Node16",
    "jsx": "react",
    "strict": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "isolatedModules": true,
    "moduleResolution": "Node16",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src",
    "app",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "extends": "expo/tsconfig.base"
}
